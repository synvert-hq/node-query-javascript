{
  "name": "synvert-core-typescript",
  "version": "1.0.0",
  "description": "write typescript code to change typescript code",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "generate": "jison -o src/parser.js src/nql.jison src/nql.jisonlex",
    "watch:generate": "nodemon --watch 'src' --ext 'jison,jisonlex' --exec 'npm run generate'",
    "test": "jest --detectOpenHandles",
    "test_watch": "jest --watch --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://git@github.com/xinminlabs/synvert-core-typescript"
  },
  "keywords": [
    "synvert",
    "typescript"
  ],
  "author": "Richard Huang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xinminlabs/synvert-core-typescript/issues"
  },
  "homepage": "https://github.com/xinminlabs/synvert-core-typescript#readme",
  "dependencies": {
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "jest": "^28.0.3",
    "jison": "^0.4.18",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.1"
  }
}
